    "georaster-layer-for-leaflet": "^3.10.0",
    "geotiff": "^2.1.3",
    "idb": "^8.0.2",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "ol": "^10.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
